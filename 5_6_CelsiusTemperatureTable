
public class CelsiusTemperatureTable {

	public static void main(String[] args) {


		for (int i = 50; i <= 212; i++) {
			
			
			
			System.out.println("Fahrenheit: " + i + "F | Celsius: " + String.format(" %,.2f" , celsius(i))+ "C");
			
			
		}
		
		
		
		
		
		
		
		

	}
	
	public static double celsius(int f)
	{
		
		double cel = (5.0/9.0)*(f - 32);
		
		return cel;
		
		
		
	}
	
	
	

}
