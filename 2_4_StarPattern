//Create a star pattern


public class StarPattern {

	public static void main(String[] args) {
		
		
		
		
		
		System.out.println("\t\t\t  *");
		System.out.println("\t\t\t ***");
		System.out.println("\t\t\t*****");
		System.out.println("\t\t\t ***");
		System.out.println("\t\t\t  *");
		
		/*
    
    	  *
			 ***
			*****
			 ***
			  *

    
    */
		
		
		
		// TODO Auto-generated method stub

	}

}
