
public class WhileLoop {

	public static void main(String[] args) {
		
		
		
		int number = 1 ;
		
		while ( number <= 5) 
		{
			
			System.out.println("You make me (sic)!");
			
			++number;
			
			
			
			
			
		}
		
		
		
		System.out.println("Done number at " + number);
		

		
		// TODO Auto-generated method stub

	}

}
/*

You make me (sic)!
You make me (sic)!
You make me (sic)!
You make me (sic)!
You make me (sic)!
Done number at 6
*/
