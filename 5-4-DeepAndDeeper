
public class DeepAndDeeper {

	public static void main(String[] args) {
		
		
		
		System.out.println("I am starting in main");
		
		
		deep();
		
		System.out.println("I am back in main.");
		

		// TODO Auto-generated method stub

	}
	
	public static void deep()
	{
		System.out.println("I am now in deep");
		deeper();
		System.out.println("I am back in deep");
	}
	
	
	public static void deeper()
	{
		System.out.println("i am now deeper.");
	}

}
/*
I am starting in main
I am now in deep
i am now deeper.
I am back in deep
I am back in main.
*/
