import java.util.Scanner;

public class Bool {

	public static void main(String[] args) {
		
		
		
		boolean flag = false;
		
		Scanner keyboard = new Scanner(System.in);
		System.out.println("Enter score:");
		double score = keyboard.nextDouble();
		
		if (score >= 90) {
			flag = true;
			
		}
		
		
		
		if (flag) {
			
			System.out.println("You have an A in the class!");
			
		}
		
	        char ch  = 'A';
		if (ch == 'A') {
	        System.out.println("The letter is A");
			
		}
		
		/*TEST RUN
Enter score:
90
You have an A in the class!
The letter is A
*/

	}

}
