
public class PassString {

	public static void main(String[] args) {
		
		
		
		String name = "Shakespeare";
		
		System.out.println("In main the name is " + name);
		
		
		changeName(name);
		
		System.out.println("Back in main it is "+name);
		


	}
	
	public static void changeName(String str)
	{
		System.out.println("Still with " + str);
		str = "Dickens";
		
		System.out.println("Changed to " + str);
	}

}
/*
In main the name is Shakespeare
Still with Shakespeare
Changed to Dickens
Back in main it is Shakespeare
*/
